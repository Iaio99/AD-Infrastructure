config:
  core.https_address: {{ cluster_address }}:8443
networks:
- name: incusbr0
  type: bridge
  config:
    ipv4.address: 172.16.252.0/22
    ipv4.dhcp: yes
    ipv4.nat: yes
storage_pools:
- name: local
driver: dir
profiles:
- name: default
  devices:
    root:
      path: /
      pool: local
      type: disk
cluster:
  server_name: {{ server_name }}
  enabled: true